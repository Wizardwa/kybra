type StatusReport = record {
  repeat : nat8;
  inline1 : nat8;
  inline2 : nat8;
  single : bool;
  inner : text;
};
type TimerIds = record {
  repeat : nat64;
  inline1 : nat64;
  inline2 : nat64;
  single : nat64;
  inner : nat64;
};
service : () -> {
  clear_timer : (nat64) -> ();
  set_timers : (nat64, nat64) -> (TimerIds);
  status_report : () -> (StatusReport) query;
}